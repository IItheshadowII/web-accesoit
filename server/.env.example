# Copy to .env and fill values

# AI Configuration
AI_API_KEY=
AI_BASE_URL=
AI_MODEL=gpt-4o

# Optional: SMTP for contact emails
SMTP_SERVICE=gmail
SMTP_USER=
SMTP_PASS=

# Database: Postgres connection string (adjust host/credentials)
DATABASE_URL="postgres://web_accesoit:PASSWORD@web-accesoit-db:5432/web_accesoit?sslmode=disable"

# n8n Webhooks (Google Calendar integration)
N8N_WEBHOOK_URL=
N8N_WEBHOOK_CANCEL_URL=
N8N_WEBHOOK_CHECK_AVAILABILITY_URL=

# Easypanel Configuration (for n8n instance provisioning)
EASYPANEL_URL=http://localhost:3000
EASYPANEL_API_KEY=
EASYPANEL_PROJECT_ID=default
MOCK_EASYPANEL=true

# Payment Gateway (Stripe or MercadoPago)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
MERCADOPAGO_ACCESS_TOKEN=
MERCADOPAGO_WEBHOOK_SECRET=